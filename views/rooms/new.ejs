<% layout('/layouts/boilerplate') %>
    <form action="/rooms" method="POST">
        <div>
            <label for="nodeId">nodeId</label>
            <input type="text" id="nodeId" name="room[nodeId]">
        </div>
        <div>
            <label for="location">Location</label>
            <input type="text" id="location" name="room[location]">
        </div>
        <div>
            <label for="title">Title</label>
            <input type="text" id="title" name="room[title]">
        </div>
        <div>
            <label for="description">Description</label>
            <input type="text" id="description" name="room[description]">
        </div>
        <div>
            <label for="zones">Zones</label>
            <input type="text" id="zones" name="room[zones]">
        </div>
        <div>
            <label for="players">Player Name</label>
            <input type="text" id="player_name" name="room[players.player_name]">
        </div>
        <div>
            <label for="players">Player ID</label>
            <input type="text" id="player_id" name="room[players.player_id]">
        </div>
        
        <button>Add Room</button>
    </form>
    <a href="/rooms">All Rooms</a>
